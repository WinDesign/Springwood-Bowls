== 'image'\">\n    </mbr-parameters>\n\n<div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div class=\"container\">\n        <div class=\"mbr-row mbr-jc-c\">\n        <div class=\" mbr-col-sm-12 mbr-col-md-6 mbr-col-lg-3\">\n                    <div class=\"inner-item rounded__7\">\n                <div class=\"image\">\n                    <amp-img src=\"@PROJECT_PATH@/assets/images/anzac-shield-506x509.jpg\" alt=\"ANZAC Shield\" height=\"220\" width=\"218.7033398821218\" layout=\"responsive\" class=\"mobirise-loader rounded__7\">\n                        <div placeholder class=\"placeholder\"></div>\n                        <img src=\"@PROJECT_PATH@/assets/images/anzac-shield-506x509.jpg\" class=\"mbr-temp rounded__7\" alt=\"ANZAC Shield\" data-aspectratio=\"1.0059288537549407\">\n                    </amp-img>\n                    <div class=\"menu-social-list align-center\" mbr-if=\"showSocial\" mbr-list mbr-list-grow=\"iconsCount\" mbr-link-type=\"socials\">\n                      <a href=\"https://www.facebook.com/pages/Mobirise/1616226671953247\" target=\"_blank\">\n                        <span mbr-icon class=\"amp-iconfont fa-facebook-f fa\"></span>\n                    </a>                    \n                    <a href=\"http://www.youtube.com/channel/UCt_tncVAetpK5JeM8L-8jyw\" target=\"_blank\">\n                        <span mbr-icon class=\"amp-iconfont fa-twitter fa\"></span>\n                    </a>\n                   \n                    \n                <a href=\"http://www.youtube.com/channel/UCt_tncVAetpK5JeM8L-8jyw\" target=\"_blank\">\n                        <span mbr-icon class=\"amp-iconfont fa-linkedin fa\"></span>\n                    </a><a href=\"http://www.youtube.com/channel/UCt_tncVAetpK5JeM8L-8jyw\" target=\"_blank\">\n                        <span mbr-icon class=\"amp-iconfont fa-vimeo-square fa\"></span>\n                    </a></div>\n                </div>\n                        <div class=\"item__text mbr-flex mbr-column align-center\">\n                    <p class=\"mbr-fonts-style name mbr-semibold mbr-black\" mbr-theme-style=\"display-7\" data-app-selector=\".name\">ANZAC Shield</p>\n                        <p class=\"signature mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".signature\">Held on or about the 25th of April the ANZAC Shield commemorates the contribution and suffering of all those who have served.<br><b><a href=\"index.html\" class=\"text-primary\"><br></a></b><br><b><a href=\"assets/files/anzac-memorial.pdf\" class=\"text-primary\" target=\"_blank\">Read article ....</a></b></p>\n                </div>\n            </div>\n        </div>\n        <div class=\"mbr-col-sm-12 mbr-col-md-6 mbr-col-lg-3\" mbr-if=\"cardsAmount>1\">\n            <div class=\"inner-item rounded__7\">\n                <div class=\"image\">\n                    <amp-img src=\"@PROJECT_PATH@/assets/images/springwood-in-springtime-506x507.jpg\" alt=\"Bowls Springwood in springtime\" height=\"220\" width=\"219.56607495069034\" layout=\"responsive\" class=\"mobirise-loader rounded__7\">\n                        <div placeholder class=\"placeholder\"></div>\n                        <img src=\"@PROJECT_PATH@/assets/images/springwood-in-springtime-506x507.jpg\" class=\"mbr-temp rounded__7\" alt=\"Bowls Springwood in springtime\" data-aspectratio=\"1.0059288537549407\">\n                    </amp-img>\n                    <div class=\"menu-social-list align-center\" mbr-if=\"showSocial\" mbr-list mbr-list-grow=\"iconsCount\" mbr-link-type=\"socials\">\n                      <a href=\"https://www.facebook.com/pages/Mobirise/1616226671953247\" target=\"_blank\">\n                        <span mbr-icon class=\"amp-iconfont fa-facebook-f fa\"></span>\n                    </a>                    \n                    <a href=\"http://www.youtube.com/channel/UCt_tncVAetpK5JeM8L-8jyw\" target=\"_blank\">\n                        <span mbr-icon class=\"amp-iconfont fa-twitter fa\"></span>\n                    </a>\n                   \n                    \n                <a href=\"http://www.youtube.com/channel/UCt_tncVAetpK5JeM8L-8jyw\" target=\"_blank\">\n                        <span mbr-icon class=\"amp-iconfont fa-linkedin fa\"></span>\n                    </a><a href=\"http://www.youtube.com/channel/UCt_tncVAetpK5JeM8L-8jyw\" target=\"_blank\">\n                        <span mbr-icon class=\"amp-iconfont fa-vimeo-square fa\"></span>\n                    </a></div>\n                </div>\n                        <div class=\"item__text mbr-flex mbr-column align-center\">\n                    <p class=\"mbr-fonts-style name mbr-semibold mbr-black\" mbr-theme-style=\"display-7\" data-app-selector=\".name\">Springwood in Springtime Bowls Tournament<br></p>\n                        <p class=\"signature mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".signature\">The premier event in our bowls calendar, the tournament attracts teams from the Western Sydney Region, adjacent areas and from further afield.<br><b><a href=\"assets/files/springwood-inspringtime.pdf\" class=\"text-primary\" target=\"_blank\">Read article ...</a></b></p>\n                </div>\n            </div>\n        </div>\n        <div class=\"mbr-col-sm-12 mbr-col-md-6 mbr-col-lg-3\" mbr-if=\"cardsAmount>2\">\n            <div class=\"inner-item rounded__7\">\n                <div class=\"image\">\n                    <amp-img src=\"@PROJECT_PATH@/assets/images/missed-community-event-506x509.jpg\" alt=\"Bowls another event\" height=\"220\" width=\"218.7033398821218\" layout=\"responsive\" class=\"mobirise-loader rounded__7\">\n                        <div placeholder class=\"placeholder\"></div>\n                        <img src=\"@PROJECT_PATH@/assets/images/missed-community-event-506x509.jpg\" class=\"mbr-temp rounded__7\" alt=\"Bowls another event\" data-aspectratio=\"1.0059288537549407\">\n             